{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          ">0.2%",
          "not dead",
          "not op_mini all",
          "last 1 chrome version",
          "last 1 firefox version",
          "last 1 safari version"
        ],
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        [
          "@babel/preset-react",
          {
            "runtime": "automatic"
          }
        ]
      ]
    }
  }
}