{
  "name": "tap-integration-platform-optimized",
  "version": "1.0.0",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@fontsource/inter": "5.2.5",
    "@mui/icons-material": "5.15.14",
    "@mui/material": "5.15.14",
    "@mui/x-date-pickers": "7.28.0",
    "axios": "1.3.4",
    "chart.js": "4.4.8",
    "date-fns": "3.6.0",
    "date-fns-tz": "3.2.0",
    "formik": "2.4.6",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.9.0",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-runtime": "7.26.9",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-react": "7.26.3",
    "@babel/runtime": "7.26.9",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "babel-loader": "9.1.3",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "6.10.0",
    "css-minimizer-webpack-plugin": "4.2.2",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "esm": "^3.2.25",
    "file-loader": "6.2.0",
    "filesize": "10.1.0",
    "html-webpack-plugin": "5.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-performance-testing": "^1.0.0",
    "mini-css-extract-plugin": "2.8.0",
    "postcss": "8.4.33",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "8.1.0",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "9.3.0",
    "react-performance-testing": "^2.0.0",
    "rimraf": "5.0.5",
    "style-loader": "3.3.4",
    "terser-webpack-plugin": "5.3.10",
    "url-loader": "4.1.1",
    "webpack": "5.98.0",
    "webpack-bundle-analyzer": "4.10.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.2.1",
    "webpack-node-externals": "3.0.0",
    "webpack-stats-plugin": "1.1.3"
  },
  "scripts": {
    "clean": "rimraf dist build bundle-reports",
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:cjs": "cross-env NODE_ENV=production WEBPACK_OUTPUT_FORMAT=cjs webpack --config config/webpack.cjs.js",
    "build:esm": "mkdir -p dist/esm && echo 'export * from \"../cjs\";' > dist/esm/index.js",
    "build:all": "npm run build:cjs && npm run build:esm",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:perf": "cross-env NODE_ENV=test jest --config jest.config.js --testMatch='**/?(*.)+(perf).[jt]s?(x)'",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "analyze": "cross-env NODE_ENV=production webpack --config config/webpack.analyzer.js",
    "analyze:report": "npm run analyze && node scripts/generate-bundle-report.js",
    "analyze:dependencies": "npm list --depth=0 && npm list --depth=0 --dev",
    "size": "npm run build && node scripts/report-bundle-size.js",
    "perf:monitor": "cross-env PERF_MONITOR=true npm start",
    "perf:profile": "cross-env NODE_ENV=production PROFILE=true webpack --config config/webpack.prod.js",
    "perf:trace": "node --trace-warnings scripts/trace-react-renders.js",
    "build:incremental": "WEBPACK_INCREMENTAL=true npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}