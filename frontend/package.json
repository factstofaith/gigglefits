    "cypress:ci:integration": "cypress run --spec 'cypress/e2e/flows/integration-creation-execution.cy.js,cypress/e2e/flows/integration-validation-errors.cy.js,cypress/e2e/flows/integration-validation-flow.cy.js,cypress/e2e/flows/data-transformation-workflow.cy.js,cypress/e2e/flows/schema-discovery-transformation.cy.js'",
    "cypress:ci:specialized": "cypress run --spec 'cypress/e2e/flows/earnings-management-workflow.cy.js,cypress/e2e/flows/azure-configuration.cy.js,cypress/e2e/flows/tenant-management.cy.js,cypress/e2e/flows/template-management.cy.js'",
    "cypress:ci:support": "cypress run --spec 'cypress/e2e/flows/documentation-portal.cy.js,cypress/e2e/flows/chat-support-help-features.cy.js,cypress/e2e/flows/security-settings-recovery.cy.js'",
    "cypress:visual": "cypress run --spec 'cypress/e2e/visual/**/*.cy.js'",
    "cypress:performance": "cypress run --spec 'cypress/e2e/performance/**/*.cy.js'",
