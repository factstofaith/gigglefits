    "test:all": "npm run test:once && npm run cypress:run && npm run lighthouse",
    "create-a11y-component": "node scripts/create-a11y-component.js",
    "bulk-enhance": "node scripts/bulk-enhance-components.js",
    "analyze-performance": "node scripts/analyze-component-performance.js",
    "a11y-audit": "node scripts/run-a11y-audit.js",
    "a11y-audit:fix": "node scripts/run-a11y-audit.js --fix",
    "a11y-audit:static": "node scripts/run-a11y-audit.js --static-only",
    "a11y-audit:dynamic": "node scripts/run-a11y-audit.js --dynamic-only",
    "test:complete": "npm run test:coverage && npm run cypress:coverage && npm run coverage:merge && npm run coverage:summary && npm run lighthouse",
    "ci:pre-deploy": "npm run validate && npm run test:ci",
    "ci:e2e": "start-server-and-test start http://localhost:3000 cypress:ci:parallel",
    "ci:component": "npm run cypress:ci:component",
    "docs": "node ../scripts/docs/generate_frontend_docs.js",
    "docs:storybook": "npm run build-storybook -- -o ../docs/generated/storybook",
    "docs:hub": "node ../scripts/docs/setup_documentation_hub.js",
    "docs:all": "npm run docs && npm run docs:storybook && npm run docs:hub",
    "build:skip-ts": "npm run clean && cross-env NODE_ENV=production SKIP_TS_CHECK=true REACT_APP_ENV=production webpack --config config/webpack.unified.js --env production",
    "restore:original-build": "npm run typecheck && react-app-rewired build",
    "restore:original-build-production": "npm run typecheck && cross-env NODE_ENV=production BUILD_TYPE=standard react-app-rewired build",
    "restore:original-build-development": "npm run typecheck && cross-env NODE_ENV=production BUILD_TYPE=development react-app-rewired build",
