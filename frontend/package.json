  "scripts": {
    "start": "node enhanced-dev-server.js",
    "build": "npm run clean && cross-env NODE_ENV=production REACT_APP_ENV=production webpack --env production && node scripts/inject-env.js",
    "dev": "npm start",
    "dev:fast": "cross-env FAST_REFRESH=true npm start",
    "verify-build": "node scripts/verify-build.js",
    "build:production": "cross-env NODE_ENV=production BUILD_TYPE=standard webpack --config config/webpack.unified.js --env production",
    "build:development": "cross-env NODE_ENV=production BUILD_TYPE=development webpack --config config/webpack.unified.js --env production",
    "build:quick": "cross-env NODE_ENV=production BUILD_TYPE=quick SKIP_TS_CHECK=true webpack --config config/webpack.unified.js --env production",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true webpack --config config/webpack.unified.js --env production",
    "build:benchmark": "cross-env NODE_ENV=production BENCHMARK=true npm run build",
    "build:incremental": "node scripts/incremental-build.js",
    "build:with-types": "npm run typecheck && npm run build",
    "build:npm-package": "npm run build:production && node scripts/verify-artifacts.js",
    "build:cjs": "cross-env WEBPACK_OUTPUT_FORMAT=cjs webpack --config config/specialized/webpack.config.cjs.cjs",
    "build:esm": "cross-env WEBPACK_OUTPUT_FORMAT=esm webpack --config config/specialized/webpack.config.esm.cjs",
    "build:all": "npm run build:cjs && npm run build:esm && npm run copy-package-files",
    "copy-package-files": "node scripts/specialized-build/copy-package-files.js",
    "fix-build-errors": "node scripts/fix-build-errors.js",
    "fix-eslint": "node scripts/fix-eslint-errors.js",
    "fix-jsx": "node scripts/fix-jsx-syntax.js",
    "fix-hooks": "node scripts/fix-react-hooks.js",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js --watch",
    "test:once": "cross-env NODE_ENV=test jest --config jest.config.js",
    "test:ci": "cross-env NODE_ENV=test CI=true jest --config jest.config.js --coverage --maxWorkers=2",
    "test:coverage": "cross-env NODE_ENV=test jest --config jest.config.js --coverage",
    "test:coverage:html": "jest --coverage --coverageReporters='html' --config=jest.config.unified.js",
    "test:coverage:summary": "jest --coverage --config=jest.config.unified.js && node scripts/generate_coverage_summary.js",
    "test:verify-coverage": "jest --coverage --config=jest.config.unified.js && node scripts/verify-coverage.js",
    "test:verify-coverage:report-only": "jest --coverage --config=jest.config.unified.js && node scripts/verify-coverage.js --report-only",
    "test:unit": "TEST_TYPE=standard jest --config=jest.config.unified.js",
    "test:utils": "TEST_TYPE=utils jest --config=jest.config.unified.js",
    "test:contexts": "TEST_TYPE=contexts jest --config=jest.config.unified.js",
    "test:integration": "TEST_TYPE=integration jest --config=jest.config.unified.js",
    "test:a11y": "TEST_TYPE=a11y jest --config=jest.config.unified.js",
    "test:watch": "jest --watch --config=jest.config.unified.js",
    "test:debug": "jest --runInBand --no-cache --config=jest.config.unified.js",
    "test:update": "jest --updateSnapshot --config=jest.config.unified.js",
    "test:cleanup": "node scripts/archive-design-system-tests.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --coverage",
