    "coverage:report": "nyc report --reporter=lcov --reporter=html --reporter=text --reporter=json-summary",
    "coverage:check": "nyc check-coverage --statements 70 --branches 60 --functions 70 --lines 70",
    "coverage:all": "npm run test:coverage && npm run cypress:coverage && npm run coverage:merge && npm run coverage:report",
    "coverage:summary": "npm run coverage:merge && node scripts/generate_coverage_summary.js",
    "lighthouse": "lighthouse http://localhost:3000 --output-path=./lighthouse-report.html --view --preset=desktop",
