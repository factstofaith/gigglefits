    "verify:artifacts": "node scripts/verify-artifacts.js",
    "verify:compatibility": "node scripts/compatibility-test.js",
    "verify:unified-report": "node scripts/generate-unified-report.js",
    "verify:all": "npm run verify:artifacts && npm run verify:compatibility && npm run verify:unified-report",
    "verify:consumption": "cypress run --spec 'cypress/e2e/npm-package/*.cy.js'",
