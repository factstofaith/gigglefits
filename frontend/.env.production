# Production environment variables for TAP Integration Platform Frontend
# These will be injected during build process

# API configuration
REACT_APP_API_URL=https://tapint-dev-backend.azurewebsites.net/api
REACT_APP_API_TIMEOUT=30000
REACT_APP_VERSION=$npm_package_version

# Feature flags
REACT_APP_ENABLE_MOCK_API=false
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_DEMO_MODE=true
REACT_APP_ENABLE_DEBUG_TOOLS=false

# Authentication
REACT_APP_AUTH_TOKEN_NAME=tap_auth_token
REACT_APP_AUTH_EXPIRY_MINUTES=1440