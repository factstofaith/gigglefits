{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "exclude": [
    "coverage/**",
    "cypress/**",
    "dist/**",
    "build/**",
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.cy.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}",
    "**/*.d.ts",
    "**/node_modules/**",
    "**/tests/**",
    "scripts/**",
    "config/**"
  ],
  "include": [
    "src/**/*.{js,jsx,ts,tsx}"
  ],
  "sourceMap": false,
  "reporter": [
    "lcov",
    "text",
    "html",
    "json-summary"
  ],
  "instrument": true,
  "statements": 70,
  "branches": 60,
  "functions": 70,
  "lines": 70
}